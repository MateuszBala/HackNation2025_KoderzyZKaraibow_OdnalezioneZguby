  "scripts": {
    "ng": "ng",
    "start-dev": "ng serve --proxy-config proxy.dev.conf.json -c local",
    "start-int": "ng serve --proxy-config proxy.int.conf.json -c local",
    "start-local": "ng serve --proxy-config proxy.local.conf.json -c local --host=0.0.0.0",
    "start-preprod": "ng serve --proxy-config proxy.preprod.conf.json -c local",
    "build": "ng build -c int",
    "build:prod": "ng build -c production",
    "build:pwa": "ng build -c pwa",
    "build:stats": "ng build --stats-json",
    "build:statsProd": "ng build -c production --stats-json",
    "bundle:report": "webpack-bundle-analyzer dist/frontend/stats.json",
