    "test:watch": "jest --watch",
    "test2": "ng test --watch=false --browsers=ChromeHeadlessNoSandbox --progress=false --code-coverage",
    "coverage": "ng test --watch=false --coverage",
    "docs:watch": "watch \"compodoc -c .compodocrc.json -s -r 4400\" src --wait=1",
    "docs": "./node_modules/.bin/compodoc -c .compodocrc.json",
    "lint": "ng lint",
    "e2e": "ng e2e",
